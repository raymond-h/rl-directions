    "prepublish": "npm run build"
  },
  "devDependencies": {
    "coffee-script": "latest",
    "coffeelint": "latest",
    "mocha": "latest",
    "chai": "latest",
    "onchange": "latest"
  },
  "dependencies": {
    "lodash": "^3.10.0"
  }
}
